{
  "expo": {
    "name": "appdis",
    "slug": "appdis",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": ["@react-native-google-signin/google-signin"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lucasbarros.appdis"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lucasbarros.appdis"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "86c66b92-a1e9-47ad-8975-ff2e1ec3f869"
      }
    }
  }
}
